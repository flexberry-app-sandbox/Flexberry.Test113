<h3 class="ui header">{{t "forms.i-i-s-test11-new-class3-e.caption"}}</h3>

<form class="ui form flexberry-vertical-form" role="form">
  {{flexberry-error error=error}}
  <div class="field">
    <div class="flexberry-edit-panel">
      {{#unless readonly}}
        <button class="ui button save-button" {{action "save"}}>
          {{t "forms.edit-form.save-button-text"}}
        </button>
        {{#unless model.isNew}}
          <button class="ui button save-del-button" {{action "delete"}}>
            {{t "forms.edit-form.delete-button-text"}}
          </button>
        {{/unless}}
      {{/unless}}
      <button class="ui button close-button" {{action "close"}}>
        {{t "forms.edit-form.close-button-text"}}
      </button>
    </div>
  </div>
  <div class="field flexberry-validationsummary-container">
    <div class="sixteen wide">
      {{flexberry-validationsummary errors=(v-get validationObject "messages")}}
    </div>
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-test11-new-class3-e.newClass5-caption"}}</label>
    {{flexberry-lookup
      choose="showLookupDialog"
      remove="removeLookupValue"
      value=model.newClass5
      relationName="newClass5"
      projection="NewClass5L"
      title=(t "forms.i-i-s-test11-new-class3-e.newClass5-caption")
      readonly=readonly
      componentName="newClass5Lookup"
      class=(if (v-get validationModel "newClass5" "isInvalid") "error")
      data-test-i-i-s-test11-new-class3-e-newClass5=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "newClass5" "message")}}
  </div>
</form>
